{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "webmeteor-www": {
      "appLocation": ".",
      "outputLocation": "dist/webmeteor-www/browser",
      "appBuildCommand": "ng build -c production",
      "run": "ng serve",
      "appDevserverUrl": "http://localhost:4200",
      "appName": "webmeteor-www",
      "resourceGroup": "Central-India-Resource-Group"
    }
  }
}